SELECT
    company_dim.name,
    job_title,
    salary_year_avg,
   job_postings_fact.company_id
FROM
    job_postings_fact
LEFT JOIN company_dim ON
company_dim.company_id = job_postings_fact.company_id
WHERE job_title_short = 'Data Analyst' AND
salary_year_avg IS NOT NULL
ORDER BY salary_year_avg DESC
limit 100;